    "format": "prettier --write '*.js' '{src,test,types}/**/*.{ts,tsx}'",
    "start": "NODE_ENV=production node src/server.js",
    "test": "run-s test:unit test:e2e",
    "test:e2e": "yarn build && scripts/test.sh",
    "test:geckodriver": "geckodriver",
    "test:unit": "NODE_ENV=test ts-node --files --project tsconfig.json node_modules/.bin/_mocha 'test/unit/**/*.{ts,tsx}'",
